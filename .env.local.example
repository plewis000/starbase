# Supabase — get these from Supabase Dashboard > Project Settings > API
NEXT_PUBLIC_SUPABASE_URL=https://ggbljtxavrjemwkqlpxg.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Plaid — get these from https://dashboard.plaid.com/developers/keys
PLAID_CLIENT_ID=your-plaid-client-id
PLAID_SECRET=your-plaid-secret
PLAID_ENV=sandbox

# Claude API — get from https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=your-anthropic-api-key

# App URL — used for Plaid webhook URL
NEXT_PUBLIC_APP_URL=https://starbase-green.vercel.app

# Discord — get from https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your-bot-token
DISCORD_APP_ID=your-application-id
DISCORD_PUBLIC_KEY=your-public-key
DISCORD_GUILD_ID=your-server-id

# Plaid webhook verification (optional but recommended)
PLAID_WEBHOOK_VERIFY_TOKEN=your-random-secret-here

# Pipeline — shared secret for worker auth
PIPELINE_SECRET=your-pipeline-secret-here
PIPELINE_CHANNEL_ID=your-discord-pipeline-channel-id

# GitHub — fine-grained token with repo scope (for merging PRs)
GITHUB_TOKEN=your-github-token-here
